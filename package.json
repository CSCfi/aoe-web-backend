    "@types/jquery": "^3.5.4",
    "@types/lodash": "^4.14.165",
    "@types/lusca": "^1.6.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.7",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.5",
    "@types/shelljs": "^0.8.8",
    "@types/supertest": "^2.0.10",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
